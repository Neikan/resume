    "css-loader": "5.0.2",
    "eslint": "7.20.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-prettier-standard": "4.0.1",
    "eslint-config-react-app": "6.0.0",
    "eslint-config-standard": "16.0.2",
    "eslint-config-standard-with-typescript": "20.0.0",
